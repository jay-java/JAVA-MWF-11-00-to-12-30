package core;
class Counter{
	private int count;
	Counter(int count){
		this.count = count;
	}
	public void countMe() {
		for(int i=1;i<=10;i++) {
			System.out.println("thread : "+count+" iteration : "+i);
		}
	}
}
public class P029_Thread {
	public static void main(String[] args) {
		Thread t = new Thread();
		System.out.println(t.currentThread());
		t.setName("MyThread");
		System.out.println(t);
		Counter c = new Counter(1);
	}
}
