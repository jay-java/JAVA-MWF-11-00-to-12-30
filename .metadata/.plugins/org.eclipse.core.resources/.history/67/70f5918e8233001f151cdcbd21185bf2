package core;
class Employee{
	int id;
	String name;
	long contact;
	double salary;
	public void setId(int id) {
		this.id = id;
	}
	public int getId() {
		return id;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getName() {
		return name;
	}
}
public class P016_Encapsulation {
	public static void main(String[] args) {
		
	}
}
