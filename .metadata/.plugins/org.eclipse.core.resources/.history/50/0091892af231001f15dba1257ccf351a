package core;
class Data{
	int a;
	String name;
	long contact;
	Data(){
		System.out.println("default cons");
	}
	Data(int a){
		this.a = a;
		System.out.println(a);
	}
	public void call() {
		System.out.println("a : "+a);
	}
	
}
public class P012_Constructor {
	public static void main(String[] args) {
		Data d1 = new Data();
		d1.call();
		Data d2 = new Data(12);
		d2.call();
	}
}
