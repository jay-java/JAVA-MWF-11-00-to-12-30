package core;

import java.util.HashMap;
import java.util.Map;

public class P027_Map {
	public static void main(String[] args) {
		Map map = new HashMap();
		map.put(1, "java");//entry
		map.put("python", 2);
		map.put(1.5, 'f');
		map.put(12, "C++");
		map.put(1, "php");
		System.out.println(map);
	}
}
